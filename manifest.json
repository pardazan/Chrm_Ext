{
    "manifest_version": 2,
    "name": "Akbar Chrome Extension",
    "version": "0.4",
    "content_scripts": [
        {
            "matches": [
                "*://apcoware.ir/*"
            ],
            "js": [
                "content.js"
            ]
        }
    ],
    "permissions": [
        "tabs"
    ],
    "browser_action": {
        "default_icon": {
            "16": "icons/apcologo_16.png",
            "48": "icons/apcologo_48.png",
            "128": "icons/apcologo_128.png"
        }
    },
    "icons": {
        "48": "icons/apcologo_48.png",
        "128": "icons/apcologo_128.png"
    }
}